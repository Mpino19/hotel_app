<div class="search-container">
  <%= form_with url: search_result_rooms_path, local: true, method: :get do |f| %>
    <%= f.text_field :area, placeholder: "エリアを入力", class: "search-box" %>
    <%= f.text_field :keyword, placeholder: "キーワードを入力", class: "search-box" %>
    <%= f.submit "検索" %>
  <% end %>
</div>
